/* components/my-tab-control/my-tab-control.wxss */
.tab-control {
  display: flex;
  font-size: 35rpx;
}

.tab-item {
  flex: 1;
  text-align: center;
  padding: 15rpx 0;
  border-bottom: 5rpx solid #eee;
}

.tab-item.active {
  color: var(--themeColor);
  border-bottom: 5rpx solid var(--themeColor);
}