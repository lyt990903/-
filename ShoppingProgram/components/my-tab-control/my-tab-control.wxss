/* components/my-tab-control/my-tab-control.wxss */
.tab-control {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #fff;
  padding-bottom: 8rpx;
}

.tab-item{
  flex: 1;
  text-align: center;
}

.active text{
  color: #ff5777;
  border-bottom: 6rpx solid #ff5777;
  padding: 20rpx 30rpx;
}

